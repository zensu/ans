#!groovy

properties([disableConcurrentBuilds()])


pipeline {
  agent {
    label 'master'
    }
  options {
    buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
    timestamps()
    }
  stages {
    stage('Simple job') {
      steps {
        sh 'ssh ec2-user@aws hostname -s'
        }
      }
    }
 }
